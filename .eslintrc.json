{
    "env": {
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "prefer-destructuring": "off",
        "no-console": "off",
        "no-undef": "off",
        "max-len": "off",
        "default-case": "off",
        "new-cap": "off",
        "no-underscore-dangle": "off"
    }
}